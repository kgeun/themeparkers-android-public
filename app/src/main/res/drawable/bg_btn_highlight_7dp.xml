<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <corners android:radius="7dp"/>
            <solid android:color="@color/highlight1_sub" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="7dp"/>
            <solid android:width="1dp" android:color="@color/basicBlack0.1"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="7dp"/>
            <stroke android:width="1dp" android:color="@color/basicBlack0.1solid"/>
        </shape>
    </item>
</selector>